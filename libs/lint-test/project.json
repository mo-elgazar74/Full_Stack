{
  "name": "lint-test",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/lint-test/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/lint-test",
        "main": "libs/lint-test/src/index.ts",
        "tsConfig": "libs/lint-test/tsconfig.lib.json",
        "assets": ["libs/lint-test/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/lint-test/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/lint-test/**/*.ts",
          "libs/lint-test/**/*.tsx",
          "libs/lint-test/**/*.js",
          "libs/lint-test/**/*.mjs"
        ]
      }
    }
  }
}
